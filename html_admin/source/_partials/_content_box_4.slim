              .os-progress-bar.primary
                .bar-labels
                  .bar-label-left
                    span Tenant C
                    /span.positive +12
                  .bar-label-right
                    span.info 124% of <b>$477</b> estimated total
                .bar-level-9[style="width: 100%"]
                  .bar-level-0[style="width: 80%"]
              .os-progress-bar.primary
                .bar-labels
                  .bar-label-left
                    span Tenant D
                    /span.positive +12
                  .bar-label-right
                    span.info 124% of  <b>$477</b> estimated total
                .bar-level-9[style="width: 100%"]
                  .bar-level-0[style="width: 80%"]
              .os-progress-bar.primary
                .bar-labels
