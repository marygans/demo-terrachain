.row
  .col-sm-12
    .element-wrapper.portfolio-header
      h6.element-header Spirit Tower Office Building
      .element-wrapper
        .chart-sorting
            .list-order
              .label From:
              select
                option April 2019
                option May 2019
                option June 2019
              .label To:
              select
                option June 2019
                option July 2019
                option August 2019
      .element-content
        .row
          .col-sm-4.col-xxxl-3
            a.element-box.el-tablo href="forms_building_breakdown.html"
              .label Free Cashflows from Operating Activities
              .value $1.86M
              .trending.trending-up-basic
                /span 12%
                /i.os-icon.os-icon-arrow-up2
              .last-updated
                / i.os-icon.os-icon-clock
                / span  1 min ago
          .col-sm-4.col-xxxl-3
            a.element-box.el-tablo href="forms_building_breakdown.html"
              .label Net Operating Income
              .value $2.33M
              .trending.trending-down-basic
                /span 12%
                /i.os-icon.os-icon-arrow-down
              .last-updated
                / i.os-icon.os-icon-clock
                / span  1 day ago
          .col-sm-4.col-xxxl-3
            a.element-box.el-tablo href="forms_building_breakdown.html"
              .label Net Operating Expenses
              .value $2.96M 
              .trending.trending-down-basic
                /span 9%
                /i.os-icon.os-icon-arrow-down
              .last-updated
                / i.os-icon.os-icon-clock
                / span  25 min ago
      .element-box
        .element-info
          .row.align-items-center
            .col-sm-8
              .element-info-with-icon
                .element-info-icon
                  .os-icon.os-icon-wallet-loaded
                .element-info-text
                  h5.element-inner-header Operating Statistics
                  .element-inner-desc Up to date info on tenant expenses in this building.
        .row
          .col-md-6
            .padded.b-l.b-r-xl
              .element-info-with-icon.smaller
                .element-info-icon
                  .os-icon.os-icon-bar-chart-stats-up
                .element-info-text
                  h5.element-inner-header Operating Expenses
                  .element-inner-desc 
                  .dashboard-last-updated
                   / i.os-icon.os-icon-clock
                    / span  3 min ago
              .os-progress-bar.blue
                .bar-labels
                  .bar-label-left
                    span.misc Utilities
                    /span.positive.header Utiliies
                    /span.positive.header Repair
                  .bar-label-right style="padding-left: 175px;"
                    span.repairs Repairs
                  .bar-label-right
                    span.info PM Costs
                  .bar-label-right style="padding-left: 0px; padding-right: 20px;"
                    span.spirit Wages
                  .bar-label-right style="padding-right: 15px;"
                    span.camex Insurance
                  .bar-label-right
                    span.standard Standard Serv.
                  .bar-label-right
                    
                .bar-level-4[style="width: 100%"]
                  .bar-level-11[style="width: 100%"]
                    .bar-level-7[style="width: 82%"]
                      .bar-level-6[style="width: 85%"]
                        .bar-level-8[style="width: 85%"]
                          .bar-level-10[style="width: 83%"]
                            .bar-level-5[style="width: 84%"]
              .os-progress-bar.blue
                .bar-labels                   
                  .bar-label-left
                    span.estimated Estimated Expenses: 2.38M
                    <br/>
                    span.positive Actual Expenses: $2.96M
                    <br/>
                    span.negative Over budget $1M
                     
          .col-md-6
            .padded.b-l.b-r-xl
              .element-info-with-icon.smaller
                .element-info-icon
                  .os-icon.os-icon-bar-chart-stats-up
                .element-info-text
                  h5.element-inner-header Tenant Expense Coverage
                  .element-inner-desc 
                  .dashboard-last-updated
                    / i.os-icon.os-icon-clock
